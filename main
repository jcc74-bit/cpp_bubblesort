#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
    srand(time(0));
    int array[100];
    for (int i = 0; i < 100; i++) {
        array[i] = i + 1;
    }
    for (int i = 0; i < 100; i++) {
        int randIndex = rand() % 100;
        int temp = array[i];
        array[i] = array[randIndex];
        array[randIndex] = temp;
    }
    cout << "random 1-100: ";
    for (int i = 0; i < 100; i++) {
        cout << array[i] << " ";
    }
    cout << "\n\n";
    for (int i = 0; i < 100 - 1; i++) {
        for (int j = 0; j < 100 - i - 1; j++) {
            if (array[j] > array[j + 1]) {
                int temp = array[j];
                array[j] = array[j + 1];
                array[j + 1] = temp;
            }
        }
    }
    cout << "bubble sort: ";
    for (int i = 0; i < 100; i++) {
        cout << array[i] << " ";
    }
    cout << "\n";
}

